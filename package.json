    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "es6-promise": "4.2.8",
    "eslint-plugin-jest": "^24.1.0",
    "express": "4.17.1",
    "express-static-gzip": "2.0.5",
    "express-winston": "4.0.1",
    "helmet": "^3.21.2",
    "http-status-codes": "1.4.0",
